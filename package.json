{
  "name": "autoeuro-api-client",
  "version": "1.0.2",
  "description": "API клиент для веб-сервиса от компании Авто-Евро",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    "./AutoeuroService": {
      "import": "./dist/esm/AutoeuroService.js",
      "require": "./dist/cjs/AutoeuroService.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf ./dist && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "start": "node dist/esm/examples/index.js",
    "start:dev": "npm run build && npm run dev",
    "lint": "npx eslint ./src",
    "format": "npx prettier --write './src/**/*.{ts,js,json}'",
    "prepare": "husky install"
  },
  "keywords": ["autoeuro", "api-client"],
  "author": "Vasiliy Rozhkov <profilrv@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/LX4777/autoeuro-api-client.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "axios": "^1.7.2"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "npx ts-node ./src/examples/index.ts"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
