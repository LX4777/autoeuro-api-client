{
  "name": "autoeuro-api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "npx nodemon",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/index.js",
    "start:dev": "npm run build && npm run dev",
    "lint": "npx eslint ./src",
    "format": "npx prettier --write './src/**/*.{ts,js,json}'",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Vasiliy Rozhkov <profilrv@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.6.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "dotenv": "^16.4.5"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": ".ts,.js",
    "ignore": [],
    "exec": "npx ts-node ./src/index.ts"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
